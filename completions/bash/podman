__podman_pod_ps() {
  local options_with_args="
   -f
   --filter
   --format
   --sort
  "

  local boolean_options="
	 --cgroup
	 --ctr-ids
	 --ctr-names
	 --ctr-status
	 --help
	 -h
	 -q
	 --quiet
	 --no-trunc
	 --labels
	 -l
	 --latest
  "
  _complete_ "$options_with_args" "$boolean_options"
}

_podman_pod_ls() {
  __podman_pod_ps
}

_podman_pod_list() {
  __podman_pod_ps
}

_podman_pod_ps() {
  __podman_pod_ps
}

