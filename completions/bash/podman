_podman_pod() {
    local boolean_options="
	--help
	-h
    "
    subcommands="
     create
     kill
     pause
     ps
     restart
     rm
     start
     stats
     stop
     top
     unpause
    "
    local aliases="
     list
     ls
    "
     __podman_subcommands "$subcommands $aliases" && return

     case "$cur" in
    -*)
	COMPREPLY=( $( compgen -W "--help" -- "$cur" ) )
	;;
    *)
	COMPREPLY=( $( compgen -W "$subcommands" -- "$cur" ) )
	;;
     esac
}

