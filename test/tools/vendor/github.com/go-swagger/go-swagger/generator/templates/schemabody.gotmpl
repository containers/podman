    {{ else }}
      {{ if not (and .IsBaseType .IsExported) }}
        {{ .GoType }}
      {{ end }}
    {{ end }}
  {{ end }}
  {{range .Properties}}
    {{ if not .IsBaseType }}
      {{ if not $.IsExported }}
        {{template "privstructfield" . }}
