      {{ pascalize .AdditionalItems.Name }}
    {{- else }}
      {{ pascalize .AdditionalItems.Name }}Field
    {{- end }} []{{ template "schemaType" .AdditionalItems }} `json:"-"`
  {{- end }}
}
