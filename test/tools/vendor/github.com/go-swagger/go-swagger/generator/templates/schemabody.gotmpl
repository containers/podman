  {{- end }}
  {{ range .Properties }}
    {{- if not $.IsExported }}
      {{- if $.IsTuple }}
        {{ template "privtuplefield" . }}
      {{- else }}
        {{ template "privstructfield" . }}
      {{- end }}
    {{- else }}
      {{- if $.IsTuple }}
        {{ template "tuplefield" . }}
