        {{- end }}
      {{- end }}
      {{- if .AdditionalItems }}
        {{- if .IsExported }}
          {{ pascalize .AdditionalItems.Name }}
        {{- else }}
          {{ .AdditionalItems.Name }}
        {{- end }} []{{ template "schemaType" .AdditionalItems }} `json:"-"`
      {{- end }}
    {{- else }}
      {{- if not (and .IsBaseType .IsExported) }}
        {{ .GoType }}
